            </element>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="(count($Get-Account-Info/resultSet/Record) > 0) and &#xA;(($Start/pfx:CreditCheckRequest/pfx:Amount + &#xA;$Get-Account-Info/resultSet/Record[1]/AMOUNT_OWED) &lt;= $Get-Account-Info/resultSet/Record[1]/AVAILABLE_CREDIT)">
                    <Status>
                        <xsl:value-of select="&quot;Accepted&quot;"/>
                    </Status>
                </xsl:when>
                <xsl:otherwise>
                    <Status>
                        <xsl:value-of select="&quot;Rejected&quot;"/>
                    </Status>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
